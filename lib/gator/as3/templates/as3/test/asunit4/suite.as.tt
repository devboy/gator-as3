package 
{
    [Suite]
    public class AllTests 
	{
        <% all_tests.each do |test| %>
        <%= "import #{test[:package_name]}.#{test[:class_name]}" %>
        <% end %>

		public function AllTests()
        {
            <% all_tests.each do |test| %>
            <%= "var #{test[:class_name].gsub(/::/, '/').
			    gsub(/([A-Z]+)([A-Z][a-z])/,'\1_\2').
			    gsub(/([a-z\d])([A-Z])/,'\1_\2').
			    tr("-", "_").
			    downcase}_#{all_tests.length}:#{test[:class_name]};" %>
            <% end %>
        }
    }
}
